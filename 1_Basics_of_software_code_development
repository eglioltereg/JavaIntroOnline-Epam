public class Main {

    // –õ–∏–Ω–µ–π–Ω—ã–µ –ø—Ä–æ–≥—Ä–∞–º–º—ã

    //–ù–∞–π–¥–∏—Ç–µ –∑–Ω–∞—á–µ–Ω–∏–µ —Ñ—É–Ω–∫—Ü–∏–∏: z = ( (a ‚Äì 3 ) * b / 2) + c.
    public static void One(){
        float  a, b, c, z;
         a=10;
         b=2;
         c=3;
         z=0;
         z = ((a - 3) * b/2 ) + c;
         System.out.println(z);
                            }

    //–í—ã—á–∏—Å–ª–∏—Ç—å –∑–Ω–∞—á–µ–Ω–∏–µ –≤—ã—Ä–∞–∂–µ–Ω–∏—è –ø–æ —Ñ–æ—Ä–º—É–ª–µ (–≤—Å–µ –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–µ –ø—Ä–∏–Ω–∏–º–∞—é—Ç –¥–µ–π—Å—Ç–≤–∏—Ç–µ–ª—å–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è):
    public static void Two(){
        double  a, b, c, z;
        a=2;
        b=1;
        c=3;
        z=0;
        z = ((b+ Math.sqrt(  Math.pow(b,2)   + 4*a*c))/(2*a)) - Math.pow(a,3)*c + Math.pow(b,-2);
        System.out.println(z);
                             }
    //–í—ã—á–∏—Å–ª–∏—Ç—å –∑–Ω–∞—á–µ–Ω–∏–µ –≤—ã—Ä–∞–∂–µ–Ω–∏—è –ø–æ —Ñ–æ—Ä–º—É–ª–µ (–≤—Å–µ –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–µ –ø—Ä–∏–Ω–∏–º–∞—é—Ç –¥–µ–π—Å—Ç–≤–∏—Ç–µ–ª—å–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è): ùë†ùëñùëõùë•+ùëêùëúùë†ùë¶ùëêùëúùë†ùë•‚àíùë†ùëñùëõùë¶‚àóùë°ùëî ùë•ùë¶
    public static void Three(){
         double x,y,z;
         x=10;
         y=5;
         z= (Math.sin(x) + Math.cos(y))/(Math.cos(x)-Math.sin(y)) * Math.tan(x*y);
        System.out.println(z);
    }

    //–î–∞–Ω–æ –¥–µ–π—Å—Ç–≤–∏—Ç–µ–ª—å–Ω–æ–µ —á–∏—Å–ª–æ R –≤–∏–¥–∞ nnn.ddd (—Ç—Ä–∏ —Ü–∏—Ñ—Ä–æ–≤—ã—Ö —Ä–∞–∑—Ä—è–¥–∞ –≤ –¥—Ä–æ–±–Ω–æ–π –∏ —Ü–µ–ª–æ–π —á–∞—Å—Ç—è—Ö). –ü–æ–º–µ–Ω—è—Ç—å –º–µ—Å—Ç–∞–º–∏ –¥—Ä–æ–±–Ω—É—é –∏ —Ü–µ–ª—É—é —á–∞—Å—Ç–∏ —á–∏—Å–ª–∞ –∏ –≤—ã–≤–µ—Å—Ç–∏ –ø–æ–ª—É—á–µ–Ω–Ω–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ —á–∏—Å–ª–∞.
    public static void Four(){
        double a = 222.533;
        String b;
        b =   String.valueOf(a);;
        System.out.println(b.substring(4) +   b.substring(3,4) + b.substring(0,3));
    }

    // –î–∞–Ω–æ –Ω–∞—Ç—É—Ä–∞–ª—å–Ω–æ–µ —á–∏—Å–ª–æ –¢, –∫–æ—Ç–æ—Ä–æ–µ –ø—Ä–µ–¥—Å—Ç–∞–≤–ª—è–µ—Ç –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å –ø—Ä–æ—à–µ–¥—à–µ–≥–æ –≤—Ä–µ–º–µ–Ω–∏ –≤ —Å–µ–∫—É–Ω–¥–∞—Ö. –í—ã–≤–µ—Å—Ç–∏ –¥–∞–Ω–Ω–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏ –≤ —á–∞—Å–∞—Ö, –º–∏–Ω—É—Ç–∞—Ö –∏ —Å–µ–∫—É–Ω–¥–∞—Ö –≤ —Å–ª–µ–¥—É—é—â–µ–π —Ñ–æ—Ä–º–µ: –ù–ù—á –ú–ú–º–∏–Ω SSc.
    public static void Five(){
        int time = 7295;
        int h,m,s = 0;
        s = Math.floorMod(time, 60);
        m = Math.floorMod(time - s, 3600)/60;
        h=  (time - s - m*60)/3600;
        System.out.println(h + "—á " + m + "–º–∏–Ω " + s + "c–µ–∫");
    }

    //–î–ª—è –¥–∞–Ω–Ω–æ–π –æ–±–ª–∞—Å—Ç–∏ —Å–æ—Å—Ç–∞–≤–∏—Ç—å –ª–∏–Ω–µ–π–Ω—É—é –ø—Ä–æ–≥—Ä–∞–º–º—É, –∫–æ—Ç–æ—Ä–∞—è –ø–µ—á–∞—Ç–∞–µ—Ç true, –µ—Å–ª–∏ —Ç–æ—á–∫–∞ —Å –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞–º–∏ (—Ö, —É) –ø—Ä–∏–Ω–∞–¥–ª–µ–∂–∏—Ç –∑–∞–∫—Ä–∞—à–µ–Ω–Ω–æ–π –æ–±–ª–∞—Å—Ç–∏, –∏ false ‚Äî –≤ –ø—Ä–æ—Ç–∏–≤–Ω–æ–º —Å–ª—É—á–∞–µ:
    public static void Six(){
          double x,y,cc;

          boolean answer = false;
          x = -0.4;
          y = -0.7;
     //   –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø—Ä–∏–Ω–∞–¥–ª–µ–∂–Ω–æ—Å—Ç–∏ –∫ –∫–≤–∞–¥—Ä–∞—Ç–∞–º
         if (-4 <= x  && x <= 4 && -3 <= y && y <= 0){answer=true;}
         if (-2 <= x  && x <= 2 &&  0 <= y && y <= 4){answer=true;}

         // –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø—Ä–∏–Ω–∞–¥–ª–µ–∂–Ω–æ—Å—Ç–∏ –∫ –∏—Å–∫–ª—é—á–µ–Ω–Ω–æ–º—É –∫—Ä—É–≥—É —á–µ—Ä–µ–∑ –≥–∏–ø–æ—Ç–µ–Ω—É–∑—É —Å–æ —Å–º–µ—â–µ–Ω–∏–µ–º -1
         if (Math.sqrt(Math.pow(x,2) + Math.pow(y+1,2)) <= 0.5) {answer=false;}

        System.out.println(answer);
    }

    // –í–µ—Ç–≤–ª–µ–Ω–∏—è

    //–î–∞–Ω—ã –¥–≤–∞ —É–≥–ª–∞ —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫–∞ (–≤ –≥—Ä–∞–¥—É—Å–∞—Ö). –û–ø—Ä–µ–¥–µ–ª–∏—Ç—å, —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –ª–∏ —Ç–∞–∫–æ–π —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫, –∏ –µ—Å–ª–∏ –¥–∞, —Ç–æ –±—É–¥–µ—Ç –ª–∏ –æ–Ω –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω—ã–º.
     public static void OneOne(){
        double alpha, beta;
        alpha = 30;
        beta = 60;
        if (alpha + beta < 180)  {System.out.print("–¢—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç");}
        else {System.out.println("–¢—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫ –Ω–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç");}
         if (alpha + beta  == 90) {System.out.println(", –∏ –æ–Ω –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω—ã–π");}

    }

    //–ù–∞–π—Ç–∏ max{min(a, b), min(c, d)}.
    public static void TwoTwo(){
        double  a,b,c,d;
        a=2; b=2; c=1; d=3;
        System.out.println("max{min(a, b), min(c, d)} = "+ Math.max( (Math.min(a,b)), (Math.min(c,d))));
        //–ï—Å–ª–∏ –Ω–µ–ª—å–∑—è –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏ —Ç–æ –ª–∏–±–æ –Ω–∞–ø–∏—Å–∞—Ç—å –ø–æ–¥—Ñ—É–Ω–∫—Ü–∏—é, –ª–∏–±–æ —Ç–∞–∫
        if (a>b){
            if(c>d){
                if (b>d){
                    System.out.println("max{min(a, b), min(c, d)} = "+ b);
                }
                else {System.out.println("max{min(a, b), min(c, d)} = "+ d);}
            }
            else {if (b>c){
                System.out.println("max{min(a, b), min(c, d)} = "+ b);
                            }
            else {System.out.println("max{min(a, b), min(c, d)} =  " + c);}}
        }
            else {
                if(c>d){
                    if (a>d){
                    System.out.println("max{min(a, b), min(c, d)} = "+ a);
                            }
                    else {System.out.println("max{min(a, b), min(c, d)} = "+ d);}
                         }
            else {if (a>c){
            System.out.println("max{min(a, b), min(c, d)} = " + a);
                          }
            else {System.out.println("max{min(a, b), min(c, d)} = "+ c);}}}

    }

    //–î–∞–Ω—ã —Ç—Ä–∏ —Ç–æ—á–∫–∏ –ê(—Ö1,—É1), –í(—Ö2,—É2) –∏ –°(—Ö3,—É3). –û–ø—Ä–µ–¥–µ–ª–∏—Ç—å, –±—É–¥—É—Ç –ª–∏ –æ–Ω–∏ —Ä–∞—Å–ø–æ–ª–æ–∂–µ–Ω—ã –Ω–∞ –æ–¥–Ω–æ–π –ø—Ä—è–º–æ–π.
    public static void ThreeThree(){
        int x1,x2,x3,y1,y2,y3,s;
        x1 = 1;
        x2 = 2;
        x3 = 3;
        y1 = 1;
        y2 = 2;
        y3 = 8;
        // —Å—á–∏—Ç–∞–µ–º —á–µ—Ä–µ–∑ –ø–ª–æ—â–∞–¥—å —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫–∞

        if ( ((x1-x3)*(y2-y3)-(x2-x3)*(y1-y3))/2 == 0 ) {System.out.println("–î–∞ –ª–µ–∂–∞—Ç –Ω–∞ –æ–¥–Ω–æ–π –ø—Ä—è–º–æ–π");}
        else {System.out.println("–ù–µ—Ç –Ω–µ –ª–µ–∂–∞—Ç –Ω–∞ –æ–¥–Ω–æ–π –ø—Ä—è–º–æ–π");}
    }

    //–ó–∞–¥–∞–Ω—ã —Ä–∞–∑–º–µ—Ä—ã –ê, –í –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω–æ–≥–æ –æ—Ç–≤–µ—Ä—Å—Ç–∏—è –∏ —Ä–∞–∑–º–µ—Ä—ã —Ö, —É, z –∫–∏—Ä–ø–∏—á–∞. –û–ø—Ä–µ–¥–µ–ª–∏—Ç—å, –ø—Ä–æ–π–¥–µ—Ç –ª–∏ –∫–∏—Ä–ø–∏—á —á–µ—Ä–µ–∑ –æ—Ç–≤–µ—Ä—Å—Ç–∏–µ.
    public static void FourFour(){
        int a,b,x,y,z;
        a=9;
        b=15;
        x=10;
        y=15;
        z=20;
        if (((a>=x) && (b>=y)) || ((a>=y) && (b>=x)) || ((a>=x) && (b>=z)) || ((a>=z) && (b>=x)) || ((a>=z)  && (b>=y))  || ((a>=y) && (b>=z))  )
           {System.out.println("–î–∞ –ø—Ä–æ–ª–µ–∑–µ—Ç");}
        else {System.out.println("–ù–µ—Ç –Ω–µ –ø—Ä–æ–ª–µ–∑–µ—Ç");}
    }

    //–í—ã—á–∏—Å–ª–∏—Ç—å –∑–Ω–∞—á–µ–Ω–∏–µ —Ñ—É–Ω–∫—Ü–∏–∏:
    public static void FiveFive(){
        double x = 4;
        if (x<=3) {System.out.println(Math.pow(x,2)-3*x+9);}
        else {System.out.println(1/(Math.pow(x,3)+6));}

    }

        //–¶–∏–∫–ª—ã

    //–ù–∞–ø–∏—à–∏—Ç–µ –ø—Ä–æ–≥—Ä–∞–º–º—É, –≥–¥–µ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å –≤–≤–æ–¥–∏—Ç –ª—é–±–æ–µ —Ü–µ–ª–æ–µ –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω–æ–µ —á–∏—Å–ª–æ. –ê –ø—Ä–æ–≥—Ä–∞–º–º–∞ —Å—É–º–º–∏—Ä—É–µ—Ç –≤—Å–µ —á–∏—Å–ª–∞ –æ—Ç 1 –¥–æ –≤–≤–µ–¥–µ–Ω–Ω–æ–≥–æ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–º —á–∏—Å–ª–∞.
    //–†–µ—à–∏–ª –≤–≤–æ–¥ –Ω–µ —Ä–µ–∞–ª–∏–∑–æ–≤—ã–≤–∞—Ç—å
    public static void OneOneOne(){
        //import java.util.Scanner;
        int x=3;

        int j=0;
        int i=0;
        while (i <= x){
              j=j+i;
              i++;
        }
        System.out.println(j);
    }

    //–í—ã—á–∏—Å–ª–∏—Ç—å –∑–Ω–∞—á–µ–Ω–∏—è —Ñ—É–Ω–∫—Ü–∏–∏ –Ω–∞ –æ—Ç—Ä–µ–∑–∫–µ [–∞,b] c —à–∞–≥–æ–º h:
    public static void TwoTwoTwo(){
        int a,b,h,x,y,i;
        a=-10;
        b=20;
        h=2;
        x=a;
        while (x<=2){
            y=-x;
            System.out.print("x = " + x + ", y = " + y + "; ");
            x = x +h;
        }
        while (x<=b){
            y=x;
            System.out.print("x = " + x + ", y = " + y + "; ");
            x = x +h;
        }
        System.out.println("");

    }

    //–ù–∞–π—Ç–∏ —Å—É–º–º—É –∫–≤–∞–¥—Ä–∞—Ç–æ–≤ –ø–µ—Ä–≤—ã—Ö —Å—Ç–∞ —á–∏—Å–µ–ª.
    public static void ThreeThreeThree(){
        int i,sum;
        sum = 0;
        for (i = 1; i <= 100; i++){
            sum = sum + i*i;
        }
        System.out.println(sum);
    }


    //–°–æ—Å—Ç–∞–≤–∏—Ç—å –ø—Ä–æ–≥—Ä–∞–º–º—É –Ω–∞—Ö–æ–∂–¥–µ–Ω–∏—è –ø—Ä–æ–∏–∑–≤–µ–¥–µ–Ω–∏—è –∫–≤–∞–¥—Ä–∞—Ç–æ–≤ –ø–µ—Ä–≤—ã—Ö –¥–≤—É—Ö—Å–æ—Ç —á–∏—Å–µ–ª.
    public static void FourFoutFour(){
        long i;
        BigInteger sum = BigInteger.valueOf(1);

        for (i =1; i <= 200; i++){
           sum = sum.multiply(BigInteger.valueOf(i*i));
        }
        System.out.println(sum);
    }

    //–î–∞–Ω—ã —á–∏—Å–ª–æ–≤–æ–π —Ä—è–¥ –∏ –Ω–µ–∫–æ—Ç–æ—Ä–æ–µ —á–∏—Å–ª–æ –µ. –ù–∞–π—Ç–∏ —Å—É–º–º—É —Ç–µ—Ö —á–ª–µ–Ω–æ–≤ —Ä—è–¥–∞, –º–æ–¥—É–ª—å –∫–æ—Ç–æ—Ä—ã—Ö –±–æ–ª—å—à–µ –∏–ª–∏ —Ä–∞–≤–µ–Ω
    //–∑–∞–¥–∞–Ω–Ω–æ–º—É –µ. –û–±—â–∏–π —á–ª–µ–Ω —Ä—è–¥–∞ –∏–º–µ–µ—Ç –≤–∏–¥:
    public static void FiveFiveFive(){
     double n, e, sum;

        e=0.001;


        sum = 0;
        n =1;
        while (Math.abs( (1/(Math.pow(2,n)))+(1/(Math.pow(3,n))) ) >=e){
            n++;
            sum = sum + Math.abs( (1/(Math.pow(2,n)))+(1/(Math.pow(3,n))) );
        }
        System.out.println(sum);
    }


    //–í—ã–≤–µ—Å—Ç–∏ –Ω–∞ —ç–∫—Ä–∞–Ω —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤–∏–π –º–µ–∂–¥—É —Å–∏–º–≤–æ–ª–∞–º–∏ –∏ –∏—Ö —á–∏—Å–ª–µ–Ω–Ω—ã–º–∏ –æ–±–æ–∑–Ω–∞—á–µ–Ω–∏—è–º–∏ –≤ –ø–∞–º—è—Ç–∏ –∫–æ–º–ø—å—é—Ç–µ—Ä–∞.
    // –ü—Ä–∏–º. –∞–≤—Ç.: –Ω–∞—á–∞–ª —Å 32-–≥–æ, –ø–æ—Ç–æ–º—É —á—Ç–æ –≤—ã—à–µ —É–ø—Ä–∞–≤–ª—è—é—â–∏–µ –∏–ª–∏ –µ—Å–∫–µ–π–ø—ã (–Ω–∞–¥–µ—é—Å—å –∑–∞–º–∞—Ä–∞—á–∏–≤–∞—Ç—å—Å—è —Å —Ö–µ–∫—Å–∞–º–∏ –Ω–µ –Ω–∞–¥–æ)
    public static void SixSixSix(){
        for (int n=32; n<=255; n++) {
            System.out.print(n + "  " + (char) n + "    ");
            if (n % 16 == 0 && n != 32){System.out.println(" ");}
                    }
        System.out.println(" ");
    }

    //–î–ª—è –∫–∞–∂–¥–æ–≥–æ –Ω–∞—Ç—É—Ä–∞–ª—å–Ω–æ–≥–æ —á–∏—Å–ª–∞ –≤ –ø—Ä–æ–º–µ–∂—É—Ç–∫–µ –æ—Ç m –¥–æ n –≤—ã–≤–µ—Å—Ç–∏ –≤—Å–µ –¥–µ–ª–∏—Ç–µ–ª–∏, –∫—Ä–æ–º–µ –µ–¥–∏–Ω–∏—Ü—ã –∏ —Å–∞–º–æ–≥–æ —á–∏—Å–ª–∞.
    //m –∏ n –≤–≤–æ–¥—è—Ç—Å—è —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã.
    public static void SevenSevenSeven() {
        int m, n, i,j;

        m = 2;
        n = 12;

        for (i = m; i <= n; i++) {
        System.out.print("–ß–∏—Å–ª–æ " + i + ";  –î–µ–ª–∏—Ç–µ–ª–∏: " );
         for (j=1; j<i; j++){
             if (i % j == 0) {System.out.print(j+ ";  " );}
         }
            System.out.println(" ");
        }

    }

    //–î–∞–Ω—ã –¥–≤–∞ —á–∏—Å–ª–∞. –û–ø—Ä–µ–¥–µ–ª–∏—Ç—å —Ü–∏—Ñ—Ä—ã, –≤—Ö–æ–¥—è—â–∏–µ –≤ –∑–∞–ø–∏—Å—å –∫–∞–∫ –ø–µ—Ä–≤–æ–≥–æ —Ç–∞–∫ –∏ –≤—Ç–æ—Ä–æ–≥–æ —á–∏—Å–ª–∞.
    public static void EightEightEight(){
        int n,m,i,j,k;
        boolean cif=false;
        n=88551322;
        m=26481655;

        boolean[] dbl = new boolean[10];


        //—Å—á–µ—Ç—á–∏–∫ n
        int nc = (n == 0) ? 1 : 0;
        int nd = n;
        while (nd != 0) {
            nc++;
            nd  /=   10;
        }

        //—Å—á–µ—Ç—á–∏–∫ m
        int mc = (m == 0) ? 1 : 0;
        int md = m;
        while (md != 0) {
            mc++;
            md /= 10;
        }

        //–≤–æ–æ–±—â–µ –ø–æ –∏–¥–µ–µ —á–µ—Ä–µ–∑ –º–Ω–æ–∂–µ—Å—Ç–≤–æ —Ç–∞–∫–æ–µ —Ä–µ—à–∞–µ—Ç—Å—è... –Ω–æ –¥–∞ –ª–∞–¥–Ω–æ
        System.out.print("–°–æ–≤–ø–∞–¥–∞—é—â–∏–µ —Ü–∏—Ñ—Ä—ã " );
        for (i =1; i <= nc; i++){
            int mt = m; //–ø–µ—Ä–µ–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ —á–∏—Å–ª–∞ m
            for (j=1; j<=mc; j++){
                if (n % 10 == mt % 10) {
                    if (dbl[n%10] == false){
                        System.out.print(n % 10 + " ");
                        dbl[n%10] = true; //—É–±–∏—Ä–∞–µ–º –¥—É–±–ª–∏
                    }
                }   mt /=  10;

            }
            n = n/10;
        }

    }


    public static void main(String[] args) {
        //–õ–∏–Ω–µ–π–Ω—ã–µ –ø—Ä–æ–≥—Ä–∞–º–º—ã

        // –ó–∞–¥–∞–Ω–∏–µ 1: –ù–∞–π–¥–∏—Ç–µ –∑–Ω–∞—á–µ–Ω–∏–µ —Ñ—É–Ω–∫—Ü–∏–∏: z = ( (a ‚Äì 3 ) * b / 2) + c.
        One();

        // –ó–∞–¥–∞–Ω–∏–µ 2: –í—ã—á–∏—Å–ª–∏—Ç—å –∑–Ω–∞—á–µ–Ω–∏–µ –≤—ã—Ä–∞–∂–µ–Ω–∏—è –ø–æ —Ñ–æ—Ä–º—É–ª–µ (–≤—Å–µ –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–µ –ø—Ä–∏–Ω–∏–º–∞—é—Ç –¥–µ–π—Å—Ç–≤–∏—Ç–µ–ª—å–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è): ùëè+‚àöùëè2+4ùëéùëê2ùëé‚àíùëé3ùëê+ùëè‚àí2
        Two();

        // –ó–∞–¥–∞–Ω–∏–µ 3: –í—ã—á–∏—Å–ª–∏—Ç—å –∑–Ω–∞—á–µ–Ω–∏–µ –≤—ã—Ä–∞–∂–µ–Ω–∏—è –ø–æ —Ñ–æ—Ä–º—É–ª–µ (–≤—Å–µ –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–µ –ø—Ä–∏–Ω–∏–º–∞—é—Ç –¥–µ–π—Å—Ç–≤–∏—Ç–µ–ª—å–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è): ùë†ùëñùëõùë•+ùëêùëúùë†ùë¶ùëêùëúùë†ùë•‚àíùë†ùëñùëõùë¶‚àóùë°ùëî ùë•ùë¶
        Three();

        // –ó–∞–¥–∞–Ω–∏–µ 4: –î–∞–Ω–æ –¥–µ–π—Å—Ç–≤–∏—Ç–µ–ª—å–Ω–æ–µ —á–∏—Å–ª–æ R –≤–∏–¥–∞ nnn.ddd (—Ç—Ä–∏ —Ü–∏—Ñ—Ä–æ–≤—ã—Ö —Ä–∞–∑—Ä—è–¥–∞ –≤ –¥—Ä–æ–±–Ω–æ–π –∏ —Ü–µ–ª–æ–π —á–∞—Å—Ç—è—Ö). –ü–æ–º–µ–Ω—è—Ç—å –º–µ—Å—Ç–∞–º–∏ –¥—Ä–æ–±–Ω—É—é –∏ —Ü–µ–ª—É—é —á–∞—Å—Ç–∏ —á–∏—Å–ª–∞ –∏ –≤—ã–≤–µ—Å—Ç–∏ –ø–æ–ª—É—á–µ–Ω–Ω–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ —á–∏—Å–ª–∞.
        Four();

        // –ó–∞–¥–∞–Ω–∏–µ 5: –î–∞–Ω–æ –Ω–∞—Ç—É—Ä–∞–ª—å–Ω–æ–µ —á–∏—Å–ª–æ –¢, –∫–æ—Ç–æ—Ä–æ–µ –ø—Ä–µ–¥—Å—Ç–∞–≤–ª—è–µ—Ç –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å –ø—Ä–æ—à–µ–¥—à–µ–≥–æ –≤—Ä–µ–º–µ–Ω–∏ –≤ —Å–µ–∫—É–Ω–¥–∞—Ö. –í—ã–≤–µ—Å—Ç–∏ –¥–∞–Ω–Ω–æ–µ –∑–Ω–∞—á–µ–Ω–∏–µ –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏ –≤ —á–∞—Å–∞—Ö, –º–∏–Ω—É—Ç–∞—Ö –∏ —Å–µ–∫—É–Ω–¥–∞—Ö –≤ —Å–ª–µ–¥—É—é—â–µ–π —Ñ–æ—Ä–º–µ –ù–ù—á –ú–ú–º–∏–Ω SSc
        Five();

        // –ó–∞–¥–∞–Ω–∏–µ 6: –î–ª—è –¥–∞–Ω–Ω–æ–π –æ–±–ª–∞—Å—Ç–∏ —Å–æ—Å—Ç–∞–≤–∏—Ç—å –ª–∏–Ω–µ–π–Ω—É—é –ø—Ä–æ–≥—Ä–∞–º–º—É, –∫–æ—Ç–æ—Ä–∞—è –ø–µ—á–∞—Ç–∞–µ—Ç true, –µ—Å–ª–∏ —Ç–æ—á–∫–∞ —Å –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞–º–∏ (—Ö, —É) –ø—Ä–∏–Ω–∞–¥–ª–µ–∂–∏—Ç –∑–∞–∫—Ä–∞—à–µ–Ω–Ω–æ–π –æ–±–ª–∞—Å—Ç–∏, –∏ false ‚Äî –≤ –ø—Ä–æ—Ç–∏–≤–Ω–æ–º —Å–ª—É—á–∞–µ:
        // –ü—Ä–∏–º. –∞–≤—Ç.: –ù–µ –¥–æ –∫–æ–Ω—Ü–∞ –ø–æ–Ω—è—Ç–Ω–∞ –∑–∞–∫—Ä–∞—à–µ–Ω–Ω–∞—è –æ–±–ª–∞—Å—Ç—å, –ø—Ä–∏–º–µ–º –µ–µ –∑–∞ –∫—Ä—É–≥ —Ä–∞–¥–∏—É—Å–æ–º 0.5 –∏ —Ü–µ–Ω—Ç—Ä–æ–º x = 0, y = -1
        Six();

            //–í–µ—Ç–≤–ª–µ–Ω–∏—è

        //–î–∞–Ω—ã –¥–≤–∞ —É–≥–ª–∞ —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫–∞ (–≤ –≥—Ä–∞–¥—É—Å–∞—Ö). –û–ø—Ä–µ–¥–µ–ª–∏—Ç—å, —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –ª–∏ —Ç–∞–∫–æ–π —Ç—Ä–µ—É–≥–æ–ª—å–Ω–∏–∫, –∏ –µ—Å–ª–∏ –¥–∞, —Ç–æ –±—É–¥–µ—Ç –ª–∏ –æ–Ω –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω—ã–º.
        OneOne();

        //–ù–∞–π—Ç–∏ max{min(a, b), min(c, d)}.
        TwoTwo();

        //–î–∞–Ω—ã —Ç—Ä–∏ —Ç–æ—á–∫–∏ –ê(—Ö1,—É1), –í(—Ö2,—É2) –∏ –°(—Ö3,—É3). –û–ø—Ä–µ–¥–µ–ª–∏—Ç—å, –±—É–¥—É—Ç –ª–∏ –æ–Ω–∏ —Ä–∞—Å–ø–æ–ª–æ–∂–µ–Ω—ã –Ω–∞ –æ–¥–Ω–æ–π –ø—Ä—è–º–æ–π.
        ThreeThree();

        //–ó–∞–¥–∞–Ω—ã —Ä–∞–∑–º–µ—Ä—ã –ê, –í –ø—Ä—è–º–æ—É–≥–æ–ª—å–Ω–æ–≥–æ –æ—Ç–≤–µ—Ä—Å—Ç–∏—è –∏ —Ä–∞–∑–º–µ—Ä—ã —Ö, —É, z –∫–∏—Ä–ø–∏—á–∞. –û–ø—Ä–µ–¥–µ–ª–∏—Ç—å, –ø—Ä–æ–π–¥–µ—Ç –ª–∏ –∫–∏—Ä–ø–∏—á —á–µ—Ä–µ–∑ –æ—Ç–≤–µ—Ä—Å—Ç–∏–µ.
        FourFour();

        // –í—ã—á–∏—Å–ª–∏—Ç—å –∑–Ω–∞—á–µ–Ω–∏–µ —Ñ—É–Ω–∫—Ü–∏–∏:
        FiveFive();



        // –¶–∏–∫–ª—ã

            // –ù–∞–ø–∏—à–∏—Ç–µ –ø—Ä–æ–≥—Ä–∞–º–º—É, –≥–¥–µ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å –≤–≤–æ–¥–∏—Ç –ª—é–±–æ–µ —Ü–µ–ª–æ–µ –ø–æ–ª–æ–∂–∏—Ç–µ–ª—å–Ω–æ–µ —á–∏—Å–ª–æ. –ê –ø—Ä–æ–≥—Ä–∞–º–º–∞ —Å—É–º–º–∏—Ä—É–µ—Ç –≤—Å–µ —á–∏—Å–ª–∞ –æ—Ç 1 –¥–æ –≤–≤–µ–¥–µ–Ω–Ω–æ–≥–æ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–º —á–∏—Å–ª–∞.
            // –ü—Ä–∏–º –∞–≤—Ç. –í–≤–æ–¥ –Ω–µ —Ä–µ–∞–ª–∏–∑–æ–≤—ã–≤–∞–ª - –ø–µ—Ä–µ–º–µ–Ω–Ω–∞—è —Ö
        OneOneOne();

            //–í—ã—á–∏—Å–ª–∏—Ç—å –∑–Ω–∞—á–µ–Ω–∏—è —Ñ—É–Ω–∫—Ü–∏–∏ –Ω–∞ –æ—Ç—Ä–µ–∑–∫–µ [–∞,b] c —à–∞–≥–æ–º h:
        TwoTwoTwo();

            //–ù–∞–π—Ç–∏ —Å—É–º–º—É –∫–≤–∞–¥—Ä–∞—Ç–æ–≤ –ø–µ—Ä–≤—ã—Ö —Å—Ç–∞ —á–∏—Å–µ–ª.
        ThreeThreeThree();

            //–°–æ—Å—Ç–∞–≤–∏—Ç—å –ø—Ä–æ–≥—Ä–∞–º–º—É –Ω–∞—Ö–æ–∂–¥–µ–Ω–∏—è –ø—Ä–æ–∏–∑–≤–µ–¥–µ–Ω–∏—è –∫–≤–∞–¥—Ä–∞—Ç–æ–≤ –ø–µ—Ä–≤—ã—Ö –¥–≤—É—Ö—Å–æ—Ç —á–∏—Å–µ–ª.
        FourFoutFour();

            //–î–∞–Ω—ã —á–∏—Å–ª–æ–≤–æ–π —Ä—è–¥ –∏ –Ω–µ–∫–æ—Ç–æ—Ä–æ–µ —á–∏—Å–ª–æ –µ. –ù–∞–π—Ç–∏ —Å—É–º–º—É —Ç–µ—Ö —á–ª–µ–Ω–æ–≤ —Ä—è–¥–∞, –º–æ–¥—É–ª—å –∫–æ—Ç–æ—Ä—ã—Ö –±–æ–ª—å—à–µ –∏–ª–∏ —Ä–∞–≤–µ–Ω
        //–∑–∞–¥–∞–Ω–Ω–æ–º—É –µ. –û–±—â–∏–π —á–ª–µ–Ω —Ä—è–¥–∞ –∏–º–µ–µ—Ç –≤–∏–¥:

        FiveFiveFive();


        // –í—ã–≤–µ—Å—Ç–∏ –Ω–∞ —ç–∫—Ä–∞–Ω —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤–∏–π –º–µ–∂–¥—É —Å–∏–º–≤–æ–ª–∞–º–∏ –∏ –∏—Ö —á–∏—Å–ª–µ–Ω–Ω—ã–º–∏ –æ–±–æ–∑–Ω–∞—á–µ–Ω–∏—è–º–∏ –≤ –ø–∞–º—è—Ç–∏ –∫–æ–º–ø—å—é—Ç–µ—Ä–∞
        SixSixSix();

        //–î–ª—è –∫–∞–∂–¥–æ–≥–æ –Ω–∞—Ç—É—Ä–∞–ª—å–Ω–æ–≥–æ —á–∏—Å–ª–∞ –≤ –ø—Ä–æ–º–µ–∂—É—Ç–∫–µ –æ—Ç m –¥–æ n –≤—ã–≤–µ—Å—Ç–∏ –≤—Å–µ –¥–µ–ª–∏—Ç–µ–ª–∏, –∫—Ä–æ–º–µ –µ–¥–∏–Ω–∏—Ü—ã –∏ —Å–∞–º–æ–≥–æ —á–∏—Å–ª–∞.
        //m –∏ n –≤–≤–æ–¥—è—Ç—Å—è —Å –∫–ª–∞–≤–∏–∞—Ç—É—Ä—ã.
        SevenSevenSeven();

        //–î–∞–Ω—ã –¥–≤–∞ —á–∏—Å–ª–∞. –û–ø—Ä–µ–¥–µ–ª–∏—Ç—å —Ü–∏—Ñ—Ä—ã, –≤—Ö–æ–¥—è—â–∏–µ –≤ –∑–∞–ø–∏—Å—å –∫–∞–∫ –ø–µ—Ä–≤–æ–≥–æ —Ç–∞–∫ –∏ –≤—Ç–æ—Ä–æ–≥–æ —á–∏—Å–ª–∞.

        EightEightEight();
          }




}
